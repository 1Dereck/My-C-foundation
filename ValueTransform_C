#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	int a, b;
	float c;
	
	printf("Digite um numero: ");
	scanf("%d", &a);
	
	b = a * 2;
	c = (float)a / 3;
	
	printf("Analisando o numero %d, seu dobro e %d e sua terca parte eh %.2f", a, b, c);
	
	return 0;
}
